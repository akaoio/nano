{
  "models": {
    "qwenvl": {
      "path": "models/qwenvl/model.rkllm",
      "type": "multimodal",
      "max_instances": 2,
      "memory_limit_mb": 8000,
      "description": "Qwen 2.5 VL multimodal model"
    },
    "lora": {
      "path": "models/lora/model.rkllm",
      "lora_path": "models/lora/lora.rkllm",
      "type": "text",
      "max_instances": 1,
      "memory_limit_mb": 4000,
      "description": "Model with LoRA adapter"
    }
  },
  "system": {
    "max_total_memory_mb": 16000,
    "max_concurrent_handles": 8,
    "worker_threads": 4,
    "request_timeout_ms": 30000,
    "response_timeout_ms": 5000
  },
  "transports": {
    "tcp": {
      "enabled": true,
      "port": 8080,
      "host": "localhost"
    },
    "udp": {
      "enabled": true,
      "port": 8081,
      "host": "localhost"
    },
    "http": {
      "enabled": true,
      "port": 8082,
      "host": "localhost",
      "path": "/rpc"
    },
    "websocket": {
      "enabled": true,
      "port": 8083,
      "host": "localhost",
      "path": "/"
    },
    "stdio": {
      "enabled": true
    }
  }
}
